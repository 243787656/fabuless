<?xml version="1.0" encoding="UTF-8"?>
<resources>

    <declare-styleable name="FabView">
        
        <attr name="fabuless_attachTo" format="reference" />
        <attr name="fabuless_attachAt" format="enum">
	        <enum name="top_left" value="1" />
	        <enum name="top_right" value="2" />
	        <enum name="bottom_left" value="3" />
	        <enum name="bottom_right" value="4" />
        </attr>
        <attr name="fabuless_attachType" format="enum">
	        <enum name="border" value="1" />
	        <enum name="inside" value="2" />
        </attr>
        <attr name="fabuless_size" format="enum">
	        <enum name="normal" value="1" />
	        <enum name="small" value="2" />
        </attr>
        <attr name="fabuless_padding" format="dimension" />
        <attr name="fabuless_revealAfterMs" format="integer" />
        
    </declare-styleable>

</resources>